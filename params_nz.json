{
    "icesat_csv": "ICESAT/nzealand/taranaki.csv",
    "features-path": "ICESAT/nzealand/rf_features",
    "interp": {
        "resolution": 100,
        "laplace": {
            "outfile": "ICESAT/nzealand/rf_features/interp_laplace.tif"
        },
        "nni": {
            "outfile": "ICESAT/nzealand/rf_features/interp_nni.tif"
        },
        "tin": {
            "outfile": "ICESAT/nzealand/rf_features/interp_tin.tif"
        },
        "idw": {
            "outfile": "ICESAT/nzealand/rf_features/interp_idw.tif",
            "power": 2,
            "n_neighbours": 10
        }
    },
    "use-model": "ICESAT/nzealand/taranaki_model.joblib",
    "results": {
        "outfile": "ICESAT/nzealand/results/taranaki_pred.tif"
    }
}